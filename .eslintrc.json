{
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "script",
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
    "rules": {
        "semi": 2,
        "no-undef": "error"
    },
    "env": {
        "browser": true
    },
    "globals": {
        "angular": false
    }
}